{
    "extends": [
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended",
        "plugin:prettier/recommended",
        "prettier",
        "plugin:gatsby/recommended"
    ],
    "plugins": ["jsx-a11y", "prettier", "gatsby"],
    "rules": {
        "prefer-const": "error",
        "prettier/prettier": ["error"],
        "gatsby/use-gatsby-link": "off"
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    }
}
